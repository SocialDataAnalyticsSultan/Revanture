package eg1;

public class Demo1 {

	public static void main(String[] args) {
		int x = 10;
		System.out.println("x = " + x);
		
		float z = 123.55f;
		System.out.println("z = " + z);
		
		long contact = 9223337790l;
		System.out.println("contct = " + contact);
		
		String s1 = "Vinay";  // Object
		System.out.println("s1: " + s1);
		
		double d = 23.445;
		System.out.println("d = " + d);
		
		/*
		 * MultiLine Comment
		 */
		
		
		
		boolean a = 6>9;
		System.out.println(a);
		
		float m = x; // implicit casting
		System.out.println("x = " + x);
		System.out.println("m = " + m);
		
		m = 89.975f;
		System.out.println("m = " + m);
		double dd = m; // implicit casting
		System.out.println("dd = " + dd);
		

	}

}
// Try Arithmetic and Relational Operations