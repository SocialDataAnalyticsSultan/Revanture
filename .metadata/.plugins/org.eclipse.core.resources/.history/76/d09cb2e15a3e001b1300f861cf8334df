		// Dynamic Memory Allocation
		Demo1 h = new Demo1();  //instantiation or object creation
		h.iAmNonStatic();
		h.iAmNonStaticAgain();
		}
	
	public static void iAmStatic() {
		System.out.println("Hello static");
	}
	
	public static void iAmStatic(String name) {
		System.out.println("Hello static by name and name = " + name);
	}
	
	public void iAmNonStatic() {
		System.out.println("Non static method of Hello Method");
	}
	
	public void iAmNonStaticAgain() {
		System.out.println("Non static again method of Hello Method");
	}
	
}

