package eg1;

public class Demo1 {

	public static void main(String[] args) {
		int x = 10;
		System.out.println("x = " + x);
		
		float z = 123.55f;
		System.out.println("z = " + z);
		
		long contact = 9223337790l;
		System.out.println("contct = " + contact);
		
		String s1 = "Vinay";
		System.out.println("s1: " + s1);
		
		double d = 23.445;
		System.out.println("d = " d);

	}

}
